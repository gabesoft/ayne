{
    "api": {
        "args": [
            "server.js",
            "--port",
            "8006"
        ],
        "color": "green",
        "resolvePath": true,
        "command": "node_modules/.bin/node-dev",
        "cwd": "../dask"
    },
    "brocco": {
        "args": [
            "serve",
            "--cors",
            "--port",
            "4300"
        ],
        "color": "magenta",
        "resolvePath": true,
        "command": "node_modules/.bin/broccoli",
        "env": { "BROCCOLI_WARN_READ_API" : "y"}
    },
    "mongo": {
        "args": [
            "--config",
            "/usr/local/etc/mongod.conf"
        ],
        "color": "green",
        "command": "mongod"
    },
    "redis": {
        "color": "yellow",
        "command": "redis-server"
    },
    "server": {
        "args": [
            "server.js",
            "--port",
            "8005"
        ],
        "color": "blue",
        "resolvePath": true,
        "command": "node_modules/.bin/node-dev"
    }
}
