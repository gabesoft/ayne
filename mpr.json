{
    "server":{
        "command": "node-dev",
        "color": "blue",
        "args" : ["server.js", "--port", "8005"],
        "env": {
            "NODE_ENV": "development"
        }
    },
    "broccoli-auth" : {
        "command": "broccoli",
        "color": "magenta",
        "args": [
            "serve",
            "--port",
            "4300"
        ],
        "cwd": "components/auth",
        "env": {
            "BROCCOLI_ENV": "development"
        }
    }
}
