{
    "server":{
        "command": "node-dev",
        "color": "blue",
        "args" : ["server.js", "--port", "8005"],
        "env": { "NODE_PATH": "$NODE_PATH:." } // TODO: implement in mpr
    },
    "brocco" : {
        "command": "broccoli",
        "color": "magenta",
        "args": [
            "serve",
            "--cors",
            "--port",
            "4300"
        ],
        "env": {}
    },
    "api":{
        "command": "node-dev",
        "color": "grey",
        "args" : ["server.js", "--port", "8006"],
        "cwd": "../dask",
        "env": {}
    },
    "mongo":{
        "command": "mongod",
        "color": "green",
        "args" : ["--config", "/usr/local/etc/mongod.conf"]
    }
}
