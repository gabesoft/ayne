{
    "api": {
        "args": [
            "server.js",
            "--port",
            "8006"
        ],
        "color": "grey",
        "command": "node-dev",
        "cwd": "../dask",
        "env": {}
    },
    "brocco": {
        "args": [
            "serve",
            "--cors",
            "--port",
            "4300"
        ],
        "color": "magenta",
        "command": "broccoli",
        "env": {}
    },
    "mongo": {
        "args": [
            "--config",
            "/usr/local/etc/mongod.conf"
        ],
        "color": "green",
        "command": "mongod"
    },
    "redis": {
        "color": "yellow",
        "command": "redis-server"
    },
    "server": {
        "args": [
            "server.js",
            "--port",
            "8005",
            "--watch",
            "**/lib/",
            "--watch",
            "**/templates/",
            "--watch",
            "server.js",
            "--ext",
            "js,jade"
        ],
        "color": "blue",
        "command": "nodemon",
        "env": {
            "NODE_PATH": "$NODE_PATH:."
        }
    }
}
