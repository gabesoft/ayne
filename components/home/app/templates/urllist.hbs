<div class="row margin-top-30">
    <div class="medium-8 large-6 columns">
        <div>
            {{query-input value=search tags=tagsData enterAction="searchUrls"}}
        </div>
    </div>
</div>
<div class="margin-bottom-30 row">
    <div class="url-list medium-8 large-6 columns">
        <div class="url-list">
            <ul class="side-nav url-list">
                {{#each url in model}}
                <li class="url-item">
                {{render 'urlview' url}}
                </li>
                {{/each}}
            </ul>
        </div>
    </div>
    <div class="medium-4 large-6 columns">
        <div class="query-list">
            <ul class="side-nav query-list">
                <li class="query-item">
                    <a href="#">#linux</a>
                </li>
                <li class="query-item">
                    <a href="#">#javascript | #ruby</a>
                </li>
            </ul>
        </div>
    </div>
</div>
