<div class="row">
    <div class="medium-10 medium-offset-1 large-8 large-offset-2 columns">
        <div class="margin-top-30">
            {{query-input value=search tags=tagsData enterAction="searchUrls"}}
        </div>
        <ul class="side-nav url-list">
            {{#each url in model}}
            <li class="url-item">
            {{render 'urlview' url}}
            </li>
            {{/each}}
        </ul>
    </div>
</div>
